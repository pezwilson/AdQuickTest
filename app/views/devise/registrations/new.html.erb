<% title "Sign Up for AdQuickTest" %>
<div class="col-sm-12">
  <div class="col-sm-10 col-md-9 col-lg-8 col-sm-offset-1 col-md-offset-2">
    <div class="page-header-registration"><h2><%= yield(:title)%></h2></div>
    <%= render 'layouts/messages' %>
  </div>
</div>
<div class="col-sm-12">
  <div class="col-sm-5 col-md-4 col-sm-offset-1 col-md-offset-2">
    <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), :html => {:class => 'form-vertical' }) do |f| %>
      <%= f.input :email, required: false %>
      <%= f.input :password, required: false %>
      <%= f.input :password_confirmation, required: false %>
      <%= f.button :submit, "Sign Up" %>
    <% end %>
  </div>
</div>
